	PADTO $0000
INT_00:
	di
	xor a
	ld i,a	; Avoid ULA ‘snow’ when I in (40, 7F]
	im 1	;
	jr RAM_TEST_JP

	PADTO $0008
INT_08:
	PADTO $0010
INT_10:
	PADTO $0018
INT_18:
	PADTO $0020
INT_20:
	PADTO $0028
INT_28:
	PADTO $0030
INT_30:
	PADTO $0038
INT_38:
	; stuff
	jp G.I_HANDLER	; Interrupt handler is in RAM to allow overrides.

RAM_TEST_JP	jp RAM_TEST